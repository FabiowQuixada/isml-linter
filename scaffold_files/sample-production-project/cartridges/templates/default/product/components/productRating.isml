<isloop items="${[1, 2, 3, 4, 5]}" var="star">
    <i class="fa
        <isif condition="${product.rating >= star}">
            fa-star
        <iselseif condition="${(product.rating % 1 > 0) && (Math.ceil(product.rating) >= star)}">
            fa-star-half-o
        <iselse>
            fa-star-o
        </isif>
        " aria-hidden="true">
    </i>
</isloop>